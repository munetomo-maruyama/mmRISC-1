FUNCTION altsyncram_cl71 (address_a[1..0], address_b[1..0], clock0, clocken0, data_a[0..0], wren_a)
RETURNS ( q_b[0..0]);
FUNCTION cntr_s3f (clk_en, clock)
RETURNS ( q[1..0]);

--synthesis_resources = lut 2 M9K 1 reg 2 
SUBDESIGN shift_taps_efl
( 
	clock	:	input;
	shiftin[0..0]	:	input;
	shiftout[0..0]	:	output;
	taps[0..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_cl71;
	cntr1 : cntr_s3f;
	clken	: NODE;

BEGIN 
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	clken = VCC;
	shiftout[0..0] = altsyncram2.q_b[0..0];
	taps[] = altsyncram2.q_b[];
END;
