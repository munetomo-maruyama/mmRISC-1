eclipse.preferences.version=1
gdb.client.commands=set mem inaccessible-by-default off\nset arch riscv\:rv32\nset remote hardware-breakpoint-limit 4\nset remote hardware-watchpoint-limit 0\nset breakpoint auto-hw on
gdb.client.doStart=true
gdb.client.executable=${cross_prefix}gdb${cross_suffix}
gdb.client.other=
gdb.openocd.doDebugInRam=false
gdb.openocd.doInitialReset=true
gdb.openocd.doPreRunReset=true
gdb.openocd.enableSemihosting=false
gdb.openocd.init.other=
gdb.openocd.initialReset.type=init
gdb.openocd.preRun.other=
gdb.openocd.preRunReset.type=halt
gdb.server.doStart=true
gdb.server.executable=/usr/local/bin/openocd
gdb.server.other=-d0 -f ../../openocd/openocd.cfg
